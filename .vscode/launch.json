{
   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "justMyCode": false,
            "program": "${workspaceFolder}/FateGrandCalculator/bin/Debug/netcoreapp3.1/FateGrandCalculator.dll",
            "args": [],
            "cwd": "${workspaceFolder}/FateGrandCalculator",
            "console": "internalConsole",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}